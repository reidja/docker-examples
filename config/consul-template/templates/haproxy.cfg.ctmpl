global
    debug
    maxconn 4
    log 127.0.0.1 local0

defaults
	log global
    mode http
	option httplog
	option dontlognull
    timeout connect 5000
    timeout client  50000
    timeout server  50000

frontend web-app
  bind *:8080
  default_backend web-app-servers

backend web-app-servers
    balance roundrobin
    {{range $i, $s := service "helloworld" }}server {{ .Name }}{{ $i }} {{ .Address }}:3000
    {{end}}